{
	propertyEditors: [
		{
  			alias: "Zone.GoogleMapsAreaPicker",
 			name: "Google Maps Area Picker",
  			editor:	{
          			view: "~/App_Plugins/Zone.GoogleMapsAreaPicker/googlemapsareapicker.html",
					valueType: "JSON"
         	},
			prevalues: {
				fields: [
					{
						label: "Default Location",
						description: "Enter the lat and long coordinates of the default location.",
						key: "defaultLocation",
						view: "textstring"
					},
					{
						label: "Default Zoom",
						description: "Enter a default zoom value",
						key: "defaultZoom",
						view: "number"
					}
				]
			}
		}
	],
	javascript:	[
 		"~/App_Plugins/Zone.GoogleMapsAreaPicker/googlemapsareapicker.controller.js"
	],
	css: [
 		"~/App_Plugins/Zone.GoogleMapsAreaPicker/googlemapsareapicker.css"
	]
}